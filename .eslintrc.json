/* eslint-disable react/prop-types */ 
{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "react/no-unescaped-entities": 0,
        "react/no-deprecated": 0,
        "react/no-direct-mutation-state": 0,
        "react/jsx-no-target-blank": 0,
        "react/jsx-key": 0
    }
}
